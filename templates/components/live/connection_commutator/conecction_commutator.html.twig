<div {{ attributes }}>
    {% embed "embeds/card.html.twig" %}
        {% block card_css_class %}mb-4{% endblock %}

        {% block card_header_css_class %}d-flex justify-content-between align-items-center{% endblock %}

        {% block card_title %}Switch{% endblock %}

        {% block card_header_options %}
            <div>
                <button class="btn btn-primary btn-sm active" aria-current="page" data-bs-toggle="modal" data-bs-target="#add-switch">
                    <i class="fas fa-plus"></i>
                    Nuevo
                </button>
            </div>
        {% endblock %}

        {% block card_body %}
            {{ component('CommutatorTable') }}
            {% if commutator %}
                <div class="row {% if isShowDetail == false %}d-none{% endif %}">
                    {{ component('CommutatorDetail', {commutator: commutator}) }}
                    {{ component('PortList', {ports: commutator.ports, selected: null}) }}
                </div>
            {% endif %}
        {% endblock %}
    {% endembed %}

    <!-- Modal -->
    {{ component('ModalForm', {
        title: 'Agregar Switch',
        src: 'NewCommutatorForm',
        vars: {
            'data-live-id': 'new-form'
        },
        type: 'component',
        id: 'add-switch'
    }) }}
    <!-- Modal -->
</div>




