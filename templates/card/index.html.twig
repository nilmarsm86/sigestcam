{% extends 'content.html.twig' %}

{% block title %}Gestionar targetas del msam: {{ msam }}{% endblock %}

{% block content %}
    <div class="row">
        <div class="col-8">
            <h3 class="text-dark mb-4">Gestionar targetas del msam: <a href="{{ path('msam_show', {'id':msam.id}) }}">{{ msam }}</a></h3>
        </div>

        {{ component('Breadcrumb', {
            options: [
                {label: 'Inicio', href: path('app_home')},
                {label: 'Targetas de Msam', href: path('card_index', {'msam':msam.id})},
                {label: 'Lista'},
            ],
            current: 'Lista'
        }) }}
    </div>

    {{ include('partials/_index_container.html.twig', {
        list: 'card/_list.html.twig',
        rest_data_action: 'page:onChangeState->content-loader#refreshContent'
    }) }}
{% endblock %}
